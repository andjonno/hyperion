/*
 * Math library.
 */
class Math {
	static square(n) {
		return n * n;
	}

	static pi() {
		return 3.14;
	}
}

class Point {
	init(x, y) {
		this.x = x;
		this.y = y;
	}

	add(point) {
		this.x = this.x + point.x;
		this.y = this.y + point.y;
	}
}

class Vector : Point {
	init(x, y, direction, speed) {
		super.init(x, y);
		this.direction = direction;
		this.speed = speed;
	}
}

var v = Vector(10, 20, 1, 2);
var p = Point(5, 10);

p.add(v);

print p.x + "," + p.y;
print 5 % 2;